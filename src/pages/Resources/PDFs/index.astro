<style>
    html{
        scroll-behavior: smooth;
    }
    body{
    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
    height:100%;
    }
    #tpye-holder{
    display: flex;
    flex-wrap: wrap;

    justify-content: space-evenly;
    align-items: center;

    text-align: center;
    }
    .PDF-Name{
    height: 90px;
    width: 100%;
    display: flex;

    color:var(--Text-color);
    font-size: 180%;

    justify-content: center;
    align-items: center;
    text-align: center;

    background-color: var(--PDF-name-BG-color);
    
    }
    .PDF-Holder img{
        transition: ease;
    }
    .PDF-Holder img:hover{
        animation: infinite Craysinp 250ms ease;
    }
    @media (orientation: portrait) {
        #A-LINKS-GROUP{
            translate: 0% 0%;
        }
        .PDF-Name{
            height: 100%;
        }
    }
    @media (orientation: landscape){
        #A-LINKS-GROUP{
            display: flex;
            translate: -10% 50%;
        }
    }
    @keyframes Craysinp{
        25%{
            transform: rotate(10deg);
        }
        50%{
            transform: rotate(0deg);
        }
        75%{
            transform: rotate(-10deg);
        }
        100%{
            transform: rotate(0deg);
        }
    }
    .PDF-Holder{
    padding: 10px;

    border: double 5px whitesmoke;
    background-clip: inherit;
    }
    iframe{
        transition: ease-out 250ms;
    }
    .PDF-Holder:hover{
        iframe{
            transform: scale(1.05);
        }    
    }
    .PDF-A-LINKS{
        display: block;
        color:black;

        padding: 5px;
        width: 50px;
        margin-bottom: 5px;

        background: white;
        border-radius: 5px;
        border: #000 solid 1px;

        text-decoration: none;

        transition: ease-in 250ms;
    }
    .PDF-A-LINKS:hover{
        background: wheat;
    }
    #A-LINKS-GROUP{
        

        width: 100%;

        justify-content: space-evenly;

        margin: 20px;
    }
    a{
        color: blue;
        text-decoration: underline;
    }
</style>